{
    "name": "recca0120/twzipcode",
    "description": "twzipcode",
    "keywords": ["twzipcode"],
    "license": "MIT",
    "type": "library",
    "require": {
        "php": ">=5.5.9",
        "ext-zlib": "*",
    },
    "require-dev": {
        "mockery/mockery": "~0.9.4",
        "phpunit/phpunit": "~4.8|~5.4",
        "nesbot/carbon": "~1.20"
    },
    "autoload": {
        "psr-4": {
            "Recca0120\\Twzipcode\\": "src"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php data/Converter.php"
        ],
        "post-install-cmd": [
            "php data/Converter.php"
        ],
        "post-update-cmd": [
            "php data/Converter.php"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
